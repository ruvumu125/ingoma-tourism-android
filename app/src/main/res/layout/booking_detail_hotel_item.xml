<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:background="@drawable/white_bg"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatTextView
        android:ellipsize="end"
        android:id="@+id/tv_hotel_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="2"
        android:text="Hotel des plateaux"
        app:layout_constraintBottom_toTopOf="@+id/tv_hotel_address"
        app:layout_constraintEnd_toStartOf="@+id/iv_booked_hotel_img"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintVertical_chainStyle="packed"
        style="@style/H2BoldTextAppearance"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textColor="@color/colorTextLight"
        android:ellipsize="end"
        android:id="@+id/tv_hotel_address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:maxLines="2"
        android:drawablePadding="8dp"
        android:lineSpacingExtra="4.76sp"
        android:text="Ajjjjj njjjj nnnn bbbbb bbbb"
        android:drawableStart="@drawable/ic_location_pin_blue"
        app:layout_constraintBottom_toTopOf="@+id/tv_hotel_address_road"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_hotel_name"
        style="@style/H3AvenirMedium"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textColor="@color/colorTextPrimary"
        android:ellipsize="end"
        android:id="@+id/tv_hotel_address_road"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:maxLines="2"
        android:text="Bujumbura-Burundi-Gitega"
        android:drawablePadding="8dp"
        android:lineSpacingExtra="4.76sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_hotel_address"
        style="@style/H5Avenir"/>
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_booked_hotel_img"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/tv_hotel_address_road"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_hotel_name"
        app:layout_constraintVertical_bias="0.5"
        app:shapeAppearanceOverlay="@style/RoundedImageView"
        app:srcCompat="@drawable/my_booking_item_hotel_icon"/>
    <View
        android:id="@+id/view23"
        android:background="@color/colorSecondaryLite"
        android:layout_width="wrap_content"
        android:layout_height="2dp" android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_hotel_address_road"/>
    <TextView
        android:textColor="@color/bookingDetailTextBlueGradient"
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Check in:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view23"
        style="@style/H5TextAppearance"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/colorTextPrimary"
        android:id="@+id/tv_check_in"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="N/A"
        android:layout_marginStart="4dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView36"
        app:layout_constraintStart_toEndOf="@+id/textView36"
        style="@style/H3BoldTextAppearance"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/colorTextPrimary"
        android:id="@+id/tv_check_out"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="N/A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view23"
        style="@style/H3BoldTextAppearance"/>
    <TextView
        android:textColor="@color/bookingDetailTextBlueGradient"
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check Out:"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView36"
        app:layout_constraintEnd_toStartOf="@+id/tv_check_out"
        app:layout_constraintTop_toTopOf="@+id/textView36"
        style="@style/H5TextAppearance"/>
    <TextView
        android:id="@+id/tv_start_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="19 May’21"
        app:fontFamily="@font/gilroy_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36"
        style="@style/H3Avenir"/>
    <TextView
        android:id="@+id/tv_end_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="19 May’21"
        app:fontFamily="@font/gilroy_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53"
        style="@style/H3Avenir"/>
    <TextView
        android:textColor="@color/colorTextPrimary"
        android:id="@+id/tv_start_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Today"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_start_date"
        style="@style/H5Avenir"/>
    <TextView
        android:textColor="@color/colorTextPrimary"
        android:id="@+id/tv_end_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Some Other Day"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_end_date"
        style="@style/H5Avenir"/>
    <TextView android:id="@+id/tv_no_of_nights"
        android:background="@drawable/button_bg_stroke_only"
        android:padding="4dp" android:paddingTop="2dp"
        android:paddingBottom="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100 Nights"
        android:paddingStart="4dp"
        android:paddingEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_start_day"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_check_in"
        style="@style/h5AvenirMedium"/>
</androidx.constraintlayout.widget.ConstraintLayout>
